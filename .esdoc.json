{
  "source": "./src",
  "destination": "./doc",
  "plugins": 
  [
    {
      "name": "esdoc-publish-html-plugin"
    },
    {
      "name": "esdoc-integrate-manual-plugin",
      "option": {
        "index": "./src/index.md",
        "globalIndex": true,
        "asset": "./src/asset",
        "files": [
          "./src/manual/formatting.md",
          "./src/manual/tutorial1.md"
        ]
      }
    }
  ]
}
